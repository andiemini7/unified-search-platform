{
  "devDependencies": {
    "node-sass": "^9.0.0",
    "tailwindcss": "^3.4.3"
  },
  "scripts": {
    "build:css": "node-sass --include-path=node_modules assets/sass/ -o assets/css/",
    "postcss": "postcss assets/css/*.css --use autoprefixer cssnano -d assets/css/"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.1",
    "postcss-cli": "^11.0.0"
  }
}
